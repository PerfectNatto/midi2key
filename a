exp_prog = export(model, example_input)

# グラフノードを巡回
for node in exp_prog.graph.nodes:
    # call_function + aten::abs を対象に
    if node.op == "call_function" and node.target == torch.ops.aten.abs:
        dtype = node.meta.get("tensor_meta", {}).get("dtype")
        # 整数型なら出力
        if dtype and "int" in str(dtype):
            print(f"Found integer abs at node {node.format_node()} with dtype={dtype}")
