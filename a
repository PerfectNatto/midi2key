int gpu_count = ncnn::get_gpu_count();
    LOGI("GPU count = %d", gpu_count);

    for (int i = 0; i < gpu_count; i++)
    {
        const ncnn::GpuInfo& gi = ncnn::get_gpu_info(i);
        LOGI("gpu[%d] name = %s",                 i, gi.device_name());
        LOGI("          type = %d  (0:other 1:integrated 2:discrete)", gi.type());
        LOGI("          vram = %d MB",            gi.memory_size / (1024*1024));
        LOGI("          compute capability = %d", gi.compute_score);
    }
