webView.get().translatesAutoresizingMaskIntoConstraints = NO;
UILayoutGuide* guide = [view get].safeAreaLayoutGuide;

[NSLayoutConstraint activateConstraints:@[
    [webView.get().leadingAnchor constraintEqualToAnchor:guide.leadingAnchor],
    [webView.get().trailingAnchor constraintEqualToAnchor:guide.trailingAnchor],
    [webView.get().topAnchor constraintEqualToAnchor:guide.topAnchor],
    [webView.get().bottomAnchor constraintEqualToAnchor:guide.bottomAnchor]
]];
